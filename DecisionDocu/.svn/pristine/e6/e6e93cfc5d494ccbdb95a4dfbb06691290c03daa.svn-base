package at.jku.se.model;

import com.fasterxml.jackson.annotation.JsonIgnore;

import at.jku.se.database.strings.RelationString;

public class Project extends Node{
	
	public Project(String name, User admin, String password){
		super(name);
		this.addRelation(RelationString.PROJECTAMIN, admin, true);
		this.addDirectProperty(RelationString.PASSWORD, password);
	}
	
	
	public Project() {
		super();
		// TODO Auto-generated constructor stub
	}

	@JsonIgnore
	public String getPassword(){
		return this.getDirectProperties().get(RelationString.PASSWORD);
	}
	@JsonIgnore
	public void setPassword(String password){
		this.addDirectProperty(RelationString.PASSWORD, password);
	}

}
